# Kubeconfig file for accessing the Kubernetes API server via Tailscale proxy
apiVersion: v1
clusters:
- cluster:
    server: https://sectorfive-api.pony-godzilla.ts.net
  name: sectorfive-api.pony-godzilla.ts.net
contexts:
- context:
    cluster: sectorfive-api.pony-godzilla.ts.net
    user: tailscale-auth
  name: sectorfive-api.pony-godzilla.ts.net
current-context: sectorfive-api.pony-godzilla.ts.net
kind: Config
users:
- name: tailscale-auth
  user:
    token: unused